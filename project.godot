;   param=value ; assign values to parameters

config_version=5

[application]

config/name="WelcomePage"
config/tags=PackedStringArray("gate")
run/main_scene="res://main/main.tscn"
run/disable_stderr=true
config/features=PackedStringArray("4.5", "Forward Plus")
run/flush_stdout_on_print=true
boot_splash/bg_color=Color(0.129412, 0.14902, 0.180392, 1)
config/icon="res://icon.png"

[audio]

buses/default_bus_layout="uid://bjsxjvg1jpdss"
driver/enable_input=true

[autoload]

FileDownloader="*res://main/loading/file_downloader.gd"
Backend="*res://main/backend/backend.gd"
FileTools="*res://main/loading/file_tools.gd"

[debug]

file_logging/enable_file_logging.pc=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/thegates_export/plugin.cfg")

